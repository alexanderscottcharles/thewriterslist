datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model Submission {
  id        String   @id @default(uuid())
  title     String
  name      String
  email     String
  createdAt DateTime? @map("created_at")
}

